        let y=1;
        document.write(x==y);
        document.write("<br>");
        document.write(x1=y);
        document.write("<br>");
        document.write(x===y);
        document.write("<br>");
        document.write(x+y);
        document.write("<br>");
        document.write(x1==y);
        document.write("<br>");
        document.write(x>y);
    </script>
