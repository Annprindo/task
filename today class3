        let a=10;
        let b=2;
        let c=a+b;
        let d=a-b;
        let e=a*b;
        let f=a/b;
        let g=a%b;
        let h=a++;
        let i=a--;
        document.write('addition operators'+ c+"<br>");
        document.write("<br>")
        document.write(d);
        document.write("<br>")
        document.write(e);
        document.write("<br>")
        document.write(f);
        document.write("<br>")
        document.write(g);
        document.write("<br>")
        document.write(h);
        document.write("<br>")
        document.write(i);
        document.write("<br>")
    </script>
